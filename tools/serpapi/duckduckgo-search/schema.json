      "default": -1,
      "description": "Filtering level for adult content (1: Strict, -1: Moderate, -2: Off)",
      "title": "Safe"
    },
    "df": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
