{
  "properties": {
    "q": {
      "description": "Query term to search in DuckDuckGo",
      "title": "Q",
      "type": "string"
    },
    "kl": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Region code for localization (e.g., 'us-en', 'uk-en', 'fr-fr')",
      "title": "Kl"
    },
    "safe": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
