{
  "tool": {
    "name": "happyrobot_get_transcript",
    "description": "Get transcript for a HappyRobot call",
    "inputSchema": {
      "properties": {
        "call_id": {
          "description": "ID of the call to get the transcript for",
          "type": "string"
        }
      },
      "required": [
        "call_id"
      ],
      "title": "GetTranscriptRequest",
      "type": "object"
    }
  },
  "language": "python",
  "auth": {
    "auth_provider": "happyrobot",
    "scopes": []
  },
  "entrypoint": {
    "build": "pip install .",
    "run": "python -m get_transcript"
  }
}